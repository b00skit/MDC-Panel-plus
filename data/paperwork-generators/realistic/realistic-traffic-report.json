{
    "id": "realistic-traffic-report",
    "title": "Traffic Citation",
    "description": "A realistic traffic infraction report.",
    "icon": "TrafficCone",
    "icon_color": "#f97316",
    "is_html_output": true,
    "custom_button_text": "Open MDC Record",
    "custom_button_link": "https://mdc.gta.world/record/{{defendant_name}}",
    "custom_button_replace_spaces": true,
    "output": "<div style=\"font-family:'Courier New',monospace;background-color:#fdf7e2;border:4px double #1c1c1c;padding:18px;color:#1a1a1a;line-height:1.35;\">\n  <div style=\"display:flex;justify-content:space-between;align-items:flex-start;margin-bottom:12px;padding-bottom:8px;border-bottom:3px double #1c1c1c;\">\n    <div>\n      <div style=\"font-size:1.25rem;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">County of Los Santos</div>\n      <div style=\"font-size:0.85rem;letter-spacing:0.08em;text-transform:uppercase;\">Department of Public Safety</div>\n    </div>\n    <div style=\"text-align:right;font-size:0.75rem;letter-spacing:0.08em;text-transform:uppercase;\">\n      <div style=\"font-weight:700;\">Form DD-214 TR</div>\n      <div>Revision {{general.date}}</div>\n      <div style=\"font-style:italic;\">Traffic Citation Record</div>\n    </div>\n  </div>\n  <table style=\"width:100%;border-collapse:collapse;\">\n    <tbody>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">1. Citation No.</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{citation_number}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">2. Citation Type</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{citation_type}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">3. Date</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{general.date}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">4. Time</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{general.time}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">5. Service Method</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{citation_service_method}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">6. Payment Due</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">On / Before {{addDays general.date 28}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;padding:6px 4px;border:none;\"><div style=\"border:2px solid #1c1c1c;border-left:none;border-right:none;padding:4px 0;margin:4px 0;background-color:#efe2bc;text-align:center;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">Officer Certification</div></td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">7. Officer</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{officers.0.name}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">8. Badge</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{officers.0.badgeNumber}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">9. Unit / Serial</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{general.callSign}} &bull; {{unit_serial_number}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">10. Division / Watch</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{officers.0.divDetail}} &bull; {{officer_watch}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">11. Officer Contact</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{officer_contact}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">12. Agency</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{officers.0.department}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;padding:6px 4px;border:none;\"><div style=\"border:2px solid #1c1c1c;border-left:none;border-right:none;padding:4px 0;margin:4px 0;background-color:#efe2bc;text-align:center;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">Defendant Identification</div></td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">13. Name</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_name}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">14. Address</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_address}}, {{defendant_city_state_zip}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">15. DOB / Age</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_dob}} ({{defendant_age}})</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">16. Sex / Race</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_sex}} / {{defendant_race}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">17. Physical Description</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">Ht {{defendant_height}} &bull; Wt {{defendant_weight}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">18. Eyes / Hair</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_eye_color}} / {{defendant_hair_color}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">19. Contact</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">Phone {{defendant_phone}} &bull; Email {{defendant_email}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">20. Employer</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_employer}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">21. License</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">State {{defendant_dl_state}} &bull; {{defendant_license_status}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">22. Signature</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{defendant_signature}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;padding:6px 4px;border:none;\"><div style=\"border:2px solid #1c1c1c;border-left:none;border-right:none;padding:4px 0;margin:4px 0;background-color:#efe2bc;text-align:center;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">Vehicle & Insurance</div></td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">23. Registration</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if vehicle_registered}}Registered{{else}}Unregistered{{/if}} &bull; {{vehicle_owner}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">24. Plate / State</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if vehicle_plate}}{{vehicle_plate}}{{else}}N/A{{/if}} / {{vehicle_registered_state}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">25. Make & Model</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{vehicle_make_model}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">26. Body / Color</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{vehicle_body_style}} / {{vehicle_color}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">27. VIN</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{vehicle_vin}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">28. Insurance</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if vehicle_insured}}Insured{{else}}Uninsured{{/if}} &bull; {{vehicle_insurance_provider}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">29. Policy / Exp.</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{vehicle_insurance_policy}} &bull; {{vehicle_insurance_expiry}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">30. Location of Stop</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{location.street}}, {{location.district}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;padding:6px 4px;border:none;\"><div style=\"border:2px solid #1c1c1c;border-left:none;border-right:none;padding:4px 0;margin:4px 0;background-color:#efe2bc;text-align:center;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">Stop Conditions & Measurements</div></td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">31. Weather</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{weather_conditions}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">32. Road Surface</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{road_conditions}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">33. Traffic Conditions</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{traffic_conditions}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">34. Control Device</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{traffic_control_device}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">35. Posted Speed</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{speed_limit}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">36. Alleged Speed</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{alleged_speed}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">37. Paced / Estimated</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{pace_speed}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">38. Calibration</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{calibration_date}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">39. Accident</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if accident_involved}}Yes{{else}}No{{/if}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">40. Injuries</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if injuries_reported}}Yes{{else}}No{{/if}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">41. Injury Details</td>\n        <td colspan=\"3\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{injury_details}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">42. Impound</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if vehicle_impounded}}Yes{{else}}No{{/if}} &bull; {{impound_reason}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">43. Impound Location</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{impound_location}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">44. License Seized</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{#if license_confiscated}}Yes{{else}}No{{/if}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">45. Confiscation Reason</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{confiscated_document}}</td>\n      </tr>\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;background-color:#f0e6c8;font-size:0.75rem;font-weight:600;letter-spacing:0.08em;text-transform:uppercase;\">46. Basis for Stop</td>\n        <td colspan=\"3\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;\">{{traffic_stop_basis}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;padding:6px 4px;border:none;\"><div style=\"border:2px solid #1c1c1c;border-left:none;border-right:none;padding:4px 0;margin:4px 0;background-color:#efe2bc;text-align:center;font-weight:700;letter-spacing:0.12em;text-transform:uppercase;\">Violation Detail</div></td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;padding:0;\"><table style=\"width:100%;border-collapse:collapse;\">\n  <tbody>\n    <tr style=\"background-color:#e9ddba;text-transform:uppercase;letter-spacing:0.08em;font-size:0.78rem;font-weight:600;\">\n      <td style=\"border:1px solid #1c1c1c;padding:6px;width:70%;\">47. Charge Description</td>\n      <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;width:15%;\">Assessment</td>\n      <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;width:15%;\">Fine (USD)</td>\n    </tr>\n    {{#any charges}}\n      {{#each charges}}\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.9rem;\">{{#with (lookup @root.penalCode chargeId)}}({{type}})({{../class}}) {{id}}. {{charge}} IC {{id}}. {{charge}}{{/with}}</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;font-size:0.9rem;\">Citation</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;font-size:0.9rem;\">${{fine}}</td>\n      </tr>\n      {{/each}}\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;font-weight:700;text-transform:uppercase;letter-spacing:0.08em;\" colspan=\"2\">Total Amount Due</td>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;text-align:right;font-weight:700;font-size:0.95rem;\">${{charges_total_fine}}</td>\n      </tr>\n    {{else}}\n      <tr>\n        <td style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.9rem;\" colspan=\"3\">No violations recorded.</td>\n      </tr>\n    {{/any}}\n  </tbody>\n</table></td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;white-space:pre-wrap;\"><strong style=\"text-transform:uppercase;letter-spacing:0.08em;\">Narrative of Events</strong><br/>{{narrative}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;white-space:pre-wrap;\"><strong style=\"text-transform:uppercase;letter-spacing:0.08em;\">Officer Actions / Warnings</strong><br/>{{officer_actions}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"2\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;white-space:pre-wrap;\"><strong style=\"text-transform:uppercase;letter-spacing:0.08em;\">Witness Statements</strong><br/>{{witness_statements}}</td>\n        <td colspan=\"2\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;white-space:pre-wrap;\"><strong style=\"text-transform:uppercase;letter-spacing:0.08em;\">Evidence Collected</strong><br/>{{evidence_collected}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.85rem;white-space:pre-wrap;background-color:#f5ebd0;\">{{compliance_instructions}}</td>\n      </tr>\n      <tr>\n        <td colspan=\"4\" style=\"border:1px solid #1c1c1c;padding:6px;font-size:0.95rem;font-size:0.8rem;background-color:#f8f1d9;font-style:italic;\">Court filings related to this citation must be initiated through the Los Santos Greg E. Joplin Courthouse prior to any scheduled hearing.</td>\n      </tr>\n    </tbody>\n  </table>\n</div>",
    "form": [
        {
            "type": "section",
            "title": "Citation Details"
        },
        {
            "type": "general",
            "name": "general"
        },
        {
            "type": "hidden",
            "name": "citation_number",
            "generateValue": {
                "type": "pattern",
                "pattern": "TC-{{date:MMM}}-{{digits:5}}",
                "uppercase": true
            }
        },
        {
            "type": "dropdown",
            "name": "citation_type",
            "label": "Citation Type",
            "placeholder": "Select citation type",
            "options": [
                "Moving Violation",
                "Non-Moving Violation",
                "Collision Related",
                "Equipment Violation"
            ]
        },
        {
            "type": "dropdown",
            "name": "citation_service_method",
            "label": "Service Method",
            "options": [
                "In Person",
                "Mailed Copy (Issued via MDC)"
            ]
        },
        {
            "type": "section",
            "title": "Officer Information"
        },
        {
            "type": "officer",
            "name": "officers",
            "showBadgeNumber": true,
            "showDivDetail": true,
            "multi": false
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "officer_watch",
                    "label": "Watch / Shift",
                    "placeholder": "e.g. Day Watch"
                },
                {
                    "type": "text",
                    "name": "officer_contact",
                    "label": "Officer Contact",
                    "placeholder": "e.g. (555) 010-2211"
                }
            ]
        },
        {
            "type": "section",
            "title": "Defendant Identification"
        },
        {
            "type": "text",
            "name": "defendant_name",
            "label": "Defendant's Full Name",
            "placeholder": "Firstname Lastname",
            "required": true
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "defendant_address",
                    "label": "Address",
                    "placeholder": "Street address"
                },
                {
                    "type": "text",
                    "name": "defendant_city_state_zip",
                    "label": "City / State / ZIP",
                    "placeholder": "Los Santos, SA 90012"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "defendant_dob",
                    "label": "Date of Birth",
                    "placeholder": "DD/MM/YYYY"
                },
                {
                    "type": "text",
                    "name": "defendant_age",
                    "label": "Age",
                    "placeholder": "Age"
                },
                {
                    "type": "text",
                    "name": "defendant_sex",
                    "label": "Sex",
                    "placeholder": "M/F/X"
                },
                {
                    "type": "text",
                    "name": "defendant_race",
                    "label": "Race",
                    "placeholder": "Race"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "defendant_height",
                    "label": "Height",
                    "placeholder": "e.g. 6'0\""
                },
                {
                    "type": "text",
                    "name": "defendant_weight",
                    "label": "Weight",
                    "placeholder": "e.g. 185 lbs"
                },
                {
                    "type": "text",
                    "name": "defendant_eye_color",
                    "label": "Eye Color",
                    "placeholder": "e.g. Brown"
                },
                {
                    "type": "text",
                    "name": "defendant_hair_color",
                    "label": "Hair Color",
                    "placeholder": "e.g. Black"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "defendant_phone",
                    "label": "Phone Number",
                    "placeholder": "e.g. (555) 010-7788"
                },
                {
                    "type": "text",
                    "name": "defendant_email",
                    "label": "Email",
                    "placeholder": "name@example.com"
                },
                {
                    "type": "text",
                    "name": "defendant_employer",
                    "label": "Employer / Occupation",
                    "placeholder": "e.g. Gruppe Sechs Security"
                },
                {
                    "type": "text",
                    "name": "defendant_signature",
                    "label": "Defendant Signature",
                    "placeholder": "If obtained"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "defendant_dl_state",
                    "label": "License State",
                    "placeholder": "SA"
                },
                {
                    "type": "dropdown",
                    "name": "defendant_license_status",
                    "label": "Driver's License Status",
                    "placeholder": "Select status",
                    "options": [
                        "Valid",
                        "No License",
                        "Expired",
                        "Suspended",
                        "Revoked"
                    ],
                    "required": true
                }
            ]
        },
        {
            "type": "section",
            "title": "Vehicle & Insurance"
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "better-switch",
                    "name": "vehicle_registered",
                    "label": "Registration Status",
                    "dataOn": "Registered",
                    "dataOff": "Unregistered",
                    "defaultValue": true
                },
                {
                    "type": "text",
                    "name": "vehicle_owner",
                    "label": "Registered Owner",
                    "placeholder": "Firstname Lastname",
                    "stipulation": {
                        "field": "vehicle_registered",
                        "value": true
                    }
                },
                {
                    "type": "text",
                    "name": "vehicle_plate",
                    "label": "Identification Plate",
                    "placeholder": "###XXX",
                    "stipulation": {
                        "field": "vehicle_registered",
                        "value": true
                    }
                },
                {
                    "type": "text",
                    "name": "vehicle_registered_state",
                    "label": "Registration State",
                    "placeholder": "SA",
                    "stipulation": {
                        "field": "vehicle_registered",
                        "value": true
                    }
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "datalist",
                    "name": "vehicle_make_model",
                    "label": "Make & Model",
                    "placeholder": "e.g. Vapid Scout",
                    "optionsSource": "vehicles",
                    "required": true
                },
                {
                    "type": "text",
                    "name": "vehicle_body_style",
                    "label": "Body Style",
                    "placeholder": "e.g. Sedan"
                },
                {
                    "type": "text",
                    "name": "vehicle_color",
                    "label": "Primary Color",
                    "placeholder": "e.g. White"
                }
            ]
        },
        {
            "type": "text",
            "name": "vehicle_vin",
            "label": "Vehicle Identification Number",
            "placeholder": "17-character VIN"
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "better-switch",
                    "name": "vehicle_insured",
                    "label": "Insurance Status",
                    "dataOn": "Insured",
                    "dataOff": "Uninsured",
                    "defaultValue": true
                },
                {
                    "type": "text",
                    "name": "vehicle_insurance_expiry",
                    "label": "Policy Expiration",
                    "placeholder": "DD/MM/YYYY",
                    "stipulation": {
                        "field": "vehicle_insured",
                        "value": true
                    }
                }
            ]
        },
        {
            "type": "section",
            "title": "Location & Conditions"
        },
        {
            "type": "location",
            "name": "location",
            "showDistrict": true
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "weather_conditions",
                    "label": "Weather Conditions",
                    "placeholder": "e.g. Clear, 70\u00b0F"
                },
                {
                    "type": "text",
                    "name": "road_conditions",
                    "label": "Road Surface",
                    "placeholder": "e.g. Dry Asphalt"
                },
                {
                    "type": "text",
                    "name": "traffic_conditions",
                    "label": "Traffic Conditions",
                    "placeholder": "e.g. Moderate"
                },
                {
                    "type": "text",
                    "name": "traffic_control_device",
                    "label": "Traffic Control Device",
                    "placeholder": "e.g. Red Light"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "speed_limit",
                    "label": "Posted Speed",
                    "placeholder": "e.g. 35 MPH"
                },
                {
                    "type": "text",
                    "name": "alleged_speed",
                    "label": "Alleged Speed",
                    "placeholder": "e.g. 52 MPH"
                },
                {
                    "type": "text",
                    "name": "pace_speed",
                    "label": "Paced/Estimated Speed",
                    "placeholder": "e.g. 50 MPH"
                }
            ]
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "calibration_date",
                    "label": "Calibration Date",
                    "placeholder": "DD/MM/YYYY"
                }
            ]
        },
        {
            "type": "better-switch",
            "name": "accident_involved",
            "label": "Accident Involved",
            "dataOn": "Yes",
            "dataOff": "No"
        },
        {
            "type": "better-switch",
            "name": "injuries_reported",
            "label": "Injuries Reported",
            "dataOn": "Yes",
            "dataOff": "No"
        },
        {
            "type": "textarea",
            "name": "injury_details",
            "label": "Injury / Collision Details",
            "placeholder": "Document any injuries, transported parties, or collision reference numbers...",
            "stipulations": [
                {
                    "field": "accident_involved",
                    "value": true
                },
                {
                    "field": "injuries_reported",
                    "value": true
                }
            ],
            "stipulationMode": "any"
        },
        {
            "type": "better-switch",
            "name": "vehicle_impounded",
            "label": "Vehicle Impounded",
            "dataOn": "Yes",
            "dataOff": "No"
        },
        {
            "type": "group",
            "fields": [
                {
                    "type": "text",
                    "name": "impound_location",
                    "label": "Impound Location",
                    "placeholder": "e.g. Mission Row Impound",
                    "stipulation": {
                        "field": "vehicle_impounded",
                        "value": true
                    }
                },
                {
                    "type": "text",
                    "name": "impound_reason",
                    "label": "Impound Reason",
                    "placeholder": "Reason",
                    "stipulation": {
                        "field": "vehicle_impounded",
                        "value": true
                    }
                }
            ]
        },
        {
            "type": "better-switch",
            "name": "license_confiscated",
            "label": "Driver License Seized",
            "dataOn": "Yes",
            "dataOff": "No"
        },
        {
            "type": "text",
            "name": "confiscated_document",
            "label": "Confiscation Reason",
            "placeholder": "Explain why the document was confiscated",
            "stipulation": {
                "field": "license_confiscated",
                "value": true
            }
        },
        {
            "type": "textarea",
            "name": "traffic_stop_basis",
            "label": "Basis for Traffic Stop",
            "placeholder": "Summarize the probable cause / reasonable suspicion leading to the stop..."
        },
        {
            "type": "section",
            "title": "Charges"
        },
        {
            "type": "charge",
            "name": "charges",
            "showClass": true,
            "showOffense": true,
            "allowedTypes": {
                "M": false,
                "I": true
            },
            "allowedIds": "401-429,435-442",
            "customFields": [
                {
                    "type": "text",
                    "name": "fine",
                    "label": "Fine",
                    "placeholder": "Fine Amount"
                }
            ],
            "previewFields": {
                "fine": true,
                "impound": true,
                "suspension": true
            }
        },
        {
            "type": "hidden",
            "name": "charges_total_fine",
            "value": "{{#with (sum (pluck charges 'fine'))}}{{this}}{{/with}}"
        },
        {
            "type": "section",
            "title": "Narrative & Evidence"
        },
        {
            "type": "textarea",
            "name": "narrative",
            "label": "Traffic Stop Narrative",
            "placeholder": "Detail the sequence of events, observed violations, and statements made by the defendant...",
            "required": true
        },
        {
            "type": "textarea",
            "name": "officer_actions",
            "label": "Officer Actions / Warnings Issued",
            "placeholder": "Document verbal warnings, field sobriety tests, searches conducted, or other actions taken."
        },
        {
            "type": "textarea",
            "name": "witness_statements",
            "label": "Witness Statements",
            "placeholder": "Summaries of any witness statements or recordings."
        },
        {
            "type": "textarea",
            "name": "evidence_collected",
            "label": "Evidence Collected",
            "placeholder": "List dash cam clips, photographs, physical evidence, or citations issued."
        },
        {
            "type": "textarea",
            "name": "compliance_instructions",
            "label": "Compliance Instructions",
            "placeholder": "Outline payment instructions, options to contest, or required corrective actions."
        },
        {
            "type": "hidden",
            "name": "vehicle_insurance_provider",
            "generateValue": {
                "type": "choice",
                "options": [
                    "Maze Bank Insurance",
                    "Fleeca Insurance"
                ]
            }
        },
        {
            "type": "hidden",
            "name": "vehicle_insurance_policy",
            "generateValue": {
                "type": "pattern",
                "pattern": "POL-{{digits:7}}"
            }
        },
        {
            "type": "hidden",
            "name": "unit_serial_number",
            "generateValue": {
                "type": "pattern",
                "pattern": "US-{{digits:6}}"
            }
        }
    ],
    "countyCityStipulation": true
}
